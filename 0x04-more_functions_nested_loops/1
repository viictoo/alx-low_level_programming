#include "main.h"

/**
  * print_square- A function that prints a square followed by a new line
  * @size: size of square
  * Return: (less than 0)
  */

void print_square(int size)
{
	int i, j;

	if (size <= 0)
	{
		_putchar('\n');
		return;
	}
	else
	{
		for (i = 0; i < size; i++)
		{
			for (j = 0; j <= size; j++)
				_putchar('#');

		_putchar('\n');
		}
	}
}
