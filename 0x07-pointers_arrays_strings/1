#include "main.h"
#include <srting.h>

/**
  * _strspn-A function that @returns: number of bytes in the initial segement
  * @s: of s which consist only of bytes from @accept
  *
  */
unsigned int _strspn(char *s, char *accept)
{
	unsigned int count = 0;
	int lena = strlen(accept);


	while (*s != ' ')
	{
		for (i = 0; i < lena; i++)
		{
			if (*s == accept[i])
				count++;
		}
		s++;
	}
	return (count);
}

